                            <radius>0.2</radius>
                        </sphere>
                    </geometry>
                </collision>
                <inertial>
                    <mass>1.0</mass>
                    <inertia>
                        <ixx>0.001</ixx>
                        <ixy>0.0</ixy>
                        <ixz>0.0</ixz>
                        <iyy>0.001</iyy>
                        <iyz>0.0</iyz>
